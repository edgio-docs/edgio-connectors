{
  "name": "@xdn/vue-storefront",
  "version": "2.45.0",
  "license": "Apache-2.0",
  "xdn-publish-to-repo": "moovweb-docs/xdn-connectors",
  "dependencies": {
    "@xdn/apollo": "^2.45.0",
    "@xdn/nuxt": "^2.45.0",
    "fs-extra": "^8.1.0",
    "jscodeshift": "^0.11.0"
  },
  "devDependencies": {
    "@xdn/core": "^2.45.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "@xdn/core": "^2.0.4"
  },
  "scripts": {
    "build": "rm -rf ./dist; mkdir ./dist; cp -r src/* package.json README.md ./dist; webpack",
    "push-build": "npm run build && cd dist && yalc push && cd ..",
    "watch": "npm-watch",
    "release": "cd ./dist; npm publish --access public",
    "release:next": "npm run release -- --tag next",
    "test": "jest --passWithNoTests",
    "precommit": "lint-staged"
  },
  "watch": {
    "push-build": {
      "patterns": [
        "src",
        "package.json",
        "webpack.config.js"
      ],
      "extensions": "*",
      "quiet": false
    }
  },
  "publishConfig": {
    "directory": "dist"
  }
}
