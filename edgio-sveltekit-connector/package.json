{
  "name": "@edgio/sveltekit",
  "version": "7.13.13",
  "license": "Apache-2.0",
  "edgio-publish-to-repo": "edgio-docs/edgio-connectors",
  "scripts": {
    "build": "rm -rf ./dist; mkdir ./dist; cp -r package.json README.md ./dist; tsc; cp -r stage/sveltekit/src/* dist;",
    "watch": "npm-watch",
    "push-build": "npm run build && cd dist && yalc push && cd ..",
    "link": "npm run build; cd dist; npm unlink; npm link",
    "release": "cd ./dist; npm publish --access public"
  },
  "watch": {
    "push-build": {
      "patterns": [
        "src",
        "package.json",
        "README.md"
      ],
      "extensions": "*",
      "quiet": false
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.12",
    "@edgio/core": "^7.13.13",
    "babel-loader": "^8.2.2"
  },
  "dependencies": {
    "@edgio/connectors": "^7.13.13"
  },
  "./adapter": {
    "types": "./adapter.d.ts",
    "import": "./adapter.js"
  }
}
